{
  "name": "solace-market",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:generate": "bunx drizzle-kit generate --config=src/database/drizzle/migrations/drizzle.config.ts",
    "db:migrate": "bunx drizzle-kit migrate --config=src/database/drizzle/migrations/drizzle.config.ts",
    "db:push": "bunx drizzle-kit push --config=src/database/drizzle/migrations/drizzle.config.ts",
    "db:studio": "bunx drizzle-kit studio --config=src/database/drizzle/migrations/drizzle.config.ts",
    "db:dump": "turso db shell your-database .dump > dump.sql",
    "db:fromDump": "cat dump.sql | sqlite3 local.db",
    "db:import:from:file": "sqlite3 local.db < src/database/drizzle/migrations/REPLACE_FILE_NAME.sql",
    "db:sync:to:json": "bun ./src/scripts/database/sync-to-json.ts",
    "db:sync:from:json": "bun ./src/scripts/database/sync-from-json.ts",
    "db:create": "bun ./src/scripts/database/create.ts",
    "db:reset": "rm -f local.db && bun run 'db:generate' && bun run 'db:create' && bun run 'db:sync:from:json'",
    "surfpool:install": "brew install txtx/taps/surfpool",
    "chain:setup": "curl --proto '=https' --tlsv1.2 -sSfL https://solana-install.solana.workers.dev | bash && bun run surfpool:install",
    "chain:setup:verify": "rustc --version && solana --version && anchor --version && node --version && bun --version",
    "chain:config:get": "solana config get",
    "chain:key:generate": "solana-keygen new",
    "chain:key:public": "solana address",
    "chain:account:balance": "solana balance",
    "chain:help": "surfpool start --help",
    "chain:start": "surfpool start",
    "chain:studio": "open http://127.0.0.1:8899",
    "create-accounts": "bun ./src/scripts/create-accounts.ts",
    "meta": "bun ./src/scripts/meta.ts",
    "dev": "next dev --turbopack",
    "build-old": "next build --turbopack",
    "build-ref": "https://github.com/vercel/next.js/issues/82881#issuecomment-3220603319",
    "build": "rm -f ./node_modules/@libsql/hrana-client/LICENSE && next build --turbopack",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "test": "vitest --config ./src/tests/vitest.config.ts",
    "test:ui": "vitest --config ./src/tests/vitest.config.ts --ui",
    "test:database": "vitest --config ./src/tests/vitest.config.ts tests/database",
    "test:dummy": "vitest --config ./src/tests/vitest.config.ts tests/database/dummy",
    "test:user-management": "vitest --config ./src/tests/vitest.config.ts tests/database/user-management",
    "test:account-management": "vitest --config ./src/tests/vitest.config.ts tests/database/account-management",
    "test:asset-management": "vitest --config ./src/tests/vitest.config.ts tests/database/asset-management",
    "test:portfolio-management": "vitest --config ./src/tests/vitest.config.ts tests/database/portfolio-management"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@solana-program/memo": "^0.9.0",
    "@solana-program/system": "^0.9.0",
    "@solana/kit": "^5.0.0",
    "@solana/react": "^5.0.0",
    "@tanstack/react-query": "^5.90.5",
    "@wallet-standard/core": "^1.1.1",
    "@wallet-standard/react": "^1.0.1",
    "@web3auth/modal": "^10.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "lightweight-charts": "^5.0.9",
    "lucide-react": "^0.548.0",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "simple-sql-parser": "^2.0.0-alpha.1",
    "sql-formatter": "^15.6.10",
    "tailwind-merge": "^3.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "@firebase/util",
    "@tailwindcss/oxide",
    "bufferutil",
    "protobufjs",
    "sharp",
    "tiny-secp256k1",
    "utf-8-validate"
  ]
}
